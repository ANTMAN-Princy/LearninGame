<div class="">
  <h1><%= link_to 'New Test', new_teacher_test_path(current_teacher) %></h1>
  <h1>Tests</h1>
    <table class="table">
        <thead>
            <tr>
                <th colspan="3">Title</th>
            </tr>
        </thead>
        <tbody>
            <% @tests.each do |tst| %>
            <tr>
              <td><%= tst.title %></td>
            </tr>
            <% end %>
        </tbody>
    </table>
</div>