<center><%= current_test.title %><br></center>

<% n = 1 %> | <%= current_test.questions.length %> |<br>
<% current_test.questions.each do |question| %>

	<%= n %>.<%= question.content %>(<%= question.points %>)<br>

	<% question.options.each do |option| %> 

		<%= option.content %><br>

	<% end %><br>

	<% n += 1 %>

<% end %>