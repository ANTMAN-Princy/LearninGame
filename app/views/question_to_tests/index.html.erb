<p id="notice"><%= notice %></p>

<h1>Question To Tests</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @question_to_tests.each do |question_to_test| %>
      <tr>
        <td><%= link_to 'Show', question_to_test %></td>
        <td><%= link_to 'Edit', edit_question_to_test_path(question_to_test) %></td>
        <td><%= link_to 'Destroy', question_to_test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Question To Test', new_question_to_test_path %>
